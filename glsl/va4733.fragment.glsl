precision lowp float;
uniform vec2 resolution;
#define time resolution
#define FAG gl_FragColor
#define F4G gl_FragCoord
#define on 1.0
#define off 0.0
#define vec5 vec4
#define onoffoffon /*onononon*/on,off,off,on
#define onononon on,on,on,on
void 
main(
){FAG 
	= F4G
.y/
		time
	.y > (on 
			  * on * on
			
			  * on * on * on
			  * on * on * on * 4.0 / 4.0 * 0.5) ? 
		vec5          (
onononon * 777.0) : 
				vec5(onoffoffon * 77777.0);
}
